.livechat-girl {
    border-radius: 50%;
    bottom: 0;
    box-shadow: 0 5px 10px 0 rgba(35, 50, 56, 0.3);
    cursor: pointer;
    height: 60px;
    opacity: 0;
    position: fixed;
    right: 40px;
    transform: translateY(0px);
    transition: all 1s cubic-bezier(0.86, 0, 0.07, 1) 0s;
    width: 60px;
    z-index: 1045;
}
.livechat-girl:focus {
    outline: 0 none;
}
.livechat-girl.animated {
    opacity: 1;
    transform: translateY(-40px);
}
.livechat-girl::after {
    background-image: linear-gradient(to bottom, #38dc79, #1ab744);
    border-radius: 50%;
    content: "";
    height: 12px;
    position: absolute;
    right: 1px;
    top: 1px;
    width: 12px;
    z-index: 50;
}

.livechat-girl .animated-circles .circle {
    background: rgba(244, 121, 33, 0.25) none repeat scroll 0 0;
    border-radius: 50%;
    height: 60px;
    position: absolute;
    transform: scale(1);
    width: 60px;
    z-index: 49;
}
.livechat-girl .animated-circles.animated .c-1 {
    animation: 2000ms cubic-bezier(0.25, 0.46, 0.45, 0.94) 0s normal forwards 1 running scaleToggleOne;
}
.livechat-girl .animated-circles.animated .c-2 {
    animation: 2500ms cubic-bezier(0.25, 0.46, 0.45, 0.94) 0s normal forwards 1 running scaleToggleTwo;
}
.livechat-girl .animated-circles.animated .c-3 {
    animation: 3000ms cubic-bezier(0.25, 0.46, 0.45, 0.94) 0s normal forwards 1 running scaleToggleThree;
}
