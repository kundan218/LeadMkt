#detailInlineInfo, #detailInlineInfo-top {
	background: #ffffff;
	border: 1px solid #dddddd;
}
#detailInlineInfo:after, #detailInlineInfo:before, #detailInlineInfo-top:after, #detailInlineInfo-top:before {
	right: 100%;
	top: 10%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
}

#detailInlineInfo:after, #detailInlineInfo-top:after {
	border-color: rgba(255, 255, 255, 0);
	border-right-color: #ffffff;
	border-width: 10px;
	margin-top: -10px;
}
#detailInlineInfo:before, #detailInlineInfo-top:before {
	border-color: rgba(221, 221, 221, 0);
	border-right-color: #dddddd;
	border-width: 11px;
	margin-top: -11px;
}
#detailInlineInfo, #detailInlineInfo-top {position: absolute; width: 300px; top: 0; background:#fff; z-index:-1;  border-radius:5px; box-shadow:0 0 10px #e9e9e9; opacity:0;}

#detailInlineInfo-top {left: 0%;}

#detailInlineInfo { right: 0%;}
#detailInlineInfo:after, #detailInlineInfo:before {
	left: 100%;
	top: 10%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
}

#detailInlineInfo:after  {
	border-color: rgba(255, 255, 255, 0);
	border-left-color: #ffffff;
	border-width: 10px;
	margin-top: -10px;
}
#detailInlineInfo:before  {
	border-color: rgba(221, 221, 221, 0);
	border-left-color: #dddddd;
	border-width: 11px;
	margin-top: -11px;
}





