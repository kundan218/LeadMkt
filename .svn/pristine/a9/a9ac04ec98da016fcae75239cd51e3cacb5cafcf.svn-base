.forGotLink { position:absolute; top:-52px; right:-1px; text-align:right; width:200px; height:50px; background-color:#1ba1e2; padding:10px 25px; color:#fff; font-size:16px;}
.forGotLink:hover { color:#fff; text-decoration:none;}

.forGotLink:before {
    content: '';
    position: absolute;
    top: 0; 
	left: 0;
    border-top: 42px solid #1D4E69;
    border-right: 40px solid #1ba1e2;
    width: 0;
}
.or { position:relative;}
.or:before {
    content: '';
    position: absolute;
    top: 50%;
    left: 0%;
    width: 100%;
    background: #ddd;
    height: 1px;
}

.or p {
    height: 40px;
    width: 40px;
    margin: 0 auto;
    line-height: 38px;
    text-align: center;
    position: relative;
    background: #ffffff;
    border: 1px solid #ddd;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    -ms-border-radius: 50%;
    border-radius: 50%;
}

